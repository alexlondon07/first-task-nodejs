{{> head }}

{{> navbar }}

<div class="jumbotron" style="padding:40px;">
    <h2>{{ title }}</h2>

    <table class="table table-hover">
        <thead class="black white-text">
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Doc</th>
                <th scope="col">Phone</th>
                <th scope="col">Username</th>
                <th scope="col">Profile</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
                <tr>
                    <td>{{this.nombre}}</td>
                    <td>{{this.doc}}</td>
                    <td>{{this.tel}}</td>
                    <td>{{this.user}}</td>
                    <td>{{this.perfil}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>